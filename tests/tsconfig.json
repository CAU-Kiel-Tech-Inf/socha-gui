{
  "compilerOptions": {
    "jsxFactory": "React.createElement",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "../out",
    "checkJs": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2016",
    "watch": false,
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "include": [
    "../src/api/**/*",
    "../src/gui/**/*",
    "../src/viewer/**/*",
    "./**/*"
  ],
  "exclude": [
  ],
  "compileOnSave": true
}
